{
  "name": "slicify",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run slice-analysis && next dev",
    "dev:no-analysis": "next dev",
    "slice-analysis": "npx tsx scripts/start-slice-analysis.ts",
    "slice-analysis:manual": "npx tsx scripts/buildSliceIndex.ts && npx tsx scripts/countSlices.ts",
