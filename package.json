    "autoprefixer": "^10.4.14",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.1",
    "postcss": "^8.4.21",
    "prettier": "2.8.7",
    "slice-machine-ui": "2.21.1",
    "tailwindcss": "^3.3.1"
  },
  "overrides": {
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3"
  }
